<launch>
    <group ns="turtle" >
        <param name="tf_prefix" value="turtlebot3" />
        <arg name="model" default="burger" doc="model type [burger, waffle, waffle_pi]"/>
        <param name="robot_description" command="$(find xacro)/xacro $(find turtlebot3_description)/urdf/turtlebot3_$(arg model).urdf.xacro" />

            <node pkg="robot_state_publisher" type="robot_state_publisher"  name="robot_state_publisher_2">
                <param name="publish_frequency" type="double" value="50.0" />
                <remap from="/turtle/joint_states" to="/joint_states" />
            </node>

            <!-- <node pkg="tf" type="static_transform_publisher" name="link2_broadcaster" args="1 0.2 -0.2 0 0 0 1 world /turtle/robot_description 100" /> -->
        <node name="trajectory" pkg="arm_motion" type="trajectory" />
    </group>
</launch>
